{
  "source": {
    "includePattern": ".+\\.js?$",
    "excludePattern" : "source\/app\/vendor\/"
  },
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts" : {
    "encoding": "utf8",
    "destination": "../dist/docs/jsdoc/",
    "recurse": true,
    "private": true,
    "lenient": false,
    "verbose": true
  }
}
